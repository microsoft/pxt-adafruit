{
    "id": "adafruitcircuitplayground",
    "nickname": "circuitplayground",
    "name": "Adafruit Circuit Playground",
    "title": "Adafruit Circuit Playground - Blocks / Javascript editor",
    "description": "A Blocks / JavaScript code editor for the Adafruit Circuit Playground.",
    "corepkg": "core",
    "bundleddirs": [
        "libs/core"
    ],
    "simulator": {
        "autoRun": true,
        "streams": true,
        "aspectRatio": 1.0,
        "parts": false, 
        "boardDefinition": {
            "visual": "circuitplayground",
            "gpioPinBlocks": [
                [
                    "P0"
                ],
                [
                    "P1"
                ],
                [
                    "P2"
                ],
                [
                    "P3"
                ],
                [
                    "P4",
                    "P5",
                    "P6",
                    "P7"
                ],
                [
                    "P8",
                    "P9",
                    "P10",
                    "P11",
                    "P12"
                ],
                [
                    "P16"
                ]
            ],
            "gpioPinMap": {
                "P0": "P0",
                "P1": "P1",
                "P2": "P2",
                "P3": "P3",
                "P4": "P4",
                "P5": "P5",
                "P6": "P6",
                "P7": "P7",
                "P8": "P8",
                "P9": "P9",
                "P10": "P10",
                "P11": "P11",
                "P12": "P12",
                "P13": "P13",
                "P14": "P14",
                "P15": "P15",
                "P16": "P16",
                "P19": "P19",
                "P20": "P20"
            },
            "i2cPins": {
                "SDA": "P2",
                "SCL": "P3"
            },
            "analogInPins": [
                "A0",
                "A4",
                "A5"
            ],
            "groundPins": [
                "GND_0", 
                "GND_1", 
                "GND_2"
            ],
            "threeVoltPins": [
                "PWR_0",
                "PWR_2"
            ],
            "attachPowerOnRight": true,
            "onboardComponents": [
                "accelerometer",
                "buttonpair",
                "ledmatrix",
                "speaker",
                "bluetooth",
                "thermometer",
                "compass"
            ],
            "useCrocClips": true,
            "marginWhenBreadboarding": [
                0,
                0,
                80,
                0
            ]
        }
    },
    "cloud": {
        "workspace": false,
        "packages": true,
        "sharing": false,
        "publish": false,
        "preferredPackages": [],
        "githubPackages": true
    },
    "blocksprj": {
        "id": "blocksprj",
        "config": {
            "name": "{0} project",
            "dependencies": {
                "core": "*"
            },
            "description": "",
            "files": [
                "main.blocks",
                "main.ts",
                "README.md"
            ]
        },
        "files": {
            "main.blocks": "<xml xmlns=\"http://www.w3.org/1999/xhtml\"></xml>",
            "main.ts": "\r\n",
            "README.md": ""
        }
    },
    "tsprj": {
        "id": "tsprj",
        "config": {
            "name": "{0} project",
            "dependencies": {
                "core": "*"
            },
            "description": "",
            "files": [
                "main.ts",
                "README.md"
            ]
        },
        "files": {
            "main.ts": "\r\n",
            "README.md": ""
        }
    },
    "compile": {
        "isNative": false,
        "nativeType": "AVR",
        "hasHex": true,
        "deployDrives": "ADAFRUIT",
        "driveName": "ADAFRUIT"
    },
    "runtime": {
        "mathBlocks": true,
        "loopsBlocks": true,
        "logicBlocks": true,
        "variablesBlocks": true
    },
    "compileService": {
        "platformioIni": [
            "[env:myenv]",
            "platform = atmelavr",
            "framework = arduino",
            "board = feather32u4"
        ],
        "buildEngine": "platformio",
        "serviceId": "circuitplayground"
    },
    "appTheme": {
        "accentColor": "#5C005C",
        "logoUrl": "https://www.adafruit.com/products/3000",
        "logo": "./static/logo.png",
        "docsLogo": "./static/logo-small.png",
        "portraitLogo": "./static/logo-small.png",
        "footerLogo": "./static/logo-small.png",
        "cardLogo": "https://az851932.vo.msecnd.net/pub/drbwxcth",
        "appLogo": "https://az851932.vo.msecnd.net/pub/tbhemtig",
        "organization": "Microsoft",
        "organizationUrl": "https://pxt.io/",
        "organizationLogo": "./static/Microsoft-logo_rgb_c-gray.png",
        "homeUrl": "https://www.adafruit.com/",
        "embedUrl": "https://www.adafruit.com/",
        "privacyUrl": "https://go.microsoft.com/fwlink/?LinkId=521839",
        "termsOfUseUrl": "https://go.microsoft.com/fwlink/?LinkID=206977",
        "githubUrl": "https://github.com/Microsoft/pxt-microbit",
        "boardName": "Adafruit Circuit Playground",
        "docMenu": [],
        "invertedMenu": true, 
        "downloadClass": "pink",
        "blockly": {
            "grid": {
                "spacing": 50,
                "length": 3,
                "colour": "#FB48C7",
                "snap": true
            }
        }
    }
}
